<%-include("../partials/header.ejs") %>



<h1>Movie List</h1>
    <table>
        <thead>
        <tr>
            <th>Title</th>
            <th>Rotten Tomatoes Score</th>
    
        </tr>
        </thead>
        <tbody>
            <% movies.forEach(function(m){ %>
            <tr>
            <td><%= m.title %></td>
            <td><%= m.rottenTomatoes %></td>
            <td><label>Saw It
                <input type="radio" name="radio">
                <label>Not Interested
                    <input type="radio" name="radio">
                <label>Want To See It
                    <input type="radio" name="radio">
                </label></td>
            </tr>
        <% }); %>






<%-include("../partials/footer.ejs") %>
