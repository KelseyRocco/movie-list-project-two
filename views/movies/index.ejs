<%-include("../partials/header.ejs") %>



<h1>Movie List</h1>
    <table>
        <thead>
        <tr>
            <th>Title</th>
            <th>Rotten Tomatoes Score</th>
            <th>Cast</th>
        </tr>
        </thead>
        <tbody>
            <% movies.forEach(function(m){ %>
            <tr>
            <td><%= m.title %></td>
            <td><%= m.rottenTomatoes %></td>
            </tr>
        <% }); %>






