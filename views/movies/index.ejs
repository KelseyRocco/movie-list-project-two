<%-include("../partials/header.ejs") %>



<h1>Movie List</h1>
    <table>
        <thead>
        <tr>
            <th>Title</th>
            <th>Rotten Tomatoes Score</th>
    
        </tr>
        </thead>
        <tbody>
            <% movies.forEach(function(m){ %>
            <tr>
            <td><%= m.title %></td>
            <td><%= m.rottenTomatoes %></td>
            <td><a href="/movies/<%= m._id %>">comments</a></td>
          
        <% }); %>


        <a href="/movies/new">Add Movie</a>






