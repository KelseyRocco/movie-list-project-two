<%-include("../partials/header.ejs") %>

<h1>Who am I watching a movie with?</h1>

        <% friends.forEach(function(f){ %>
        <ul>
            <a href="/friends/<%= f._id %>">
            <li><%= f.firstName %> <%= f.lastName %></li>
        
            <form action="<= f._id %>?_method=DELETE" method="POST">
                <button type="submit">Bye</button>
            </form>

            </a>
            

        </ul>
        <% }); %>



<%-include("../partials/footer.ejs") %>
